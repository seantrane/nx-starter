{
  "name": "@seantrane/nx-starter",
  "description": "An Nx Workspace starter kit for managing Angular apps and their shared libraries.",
  "version": "1.0.0",
  "author": "@seantrane",
  "license": "ISC",
  "homepage": "https://github.com/seantrane/nx-starter#readme",
  "repository": "github:seantrane/nx-starter",
  "bugs": "https://github.com/seantrane/nx-starter/issues",
  "private": true,
  "scripts": {
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "build": "ng build",
    "ci:all": "npm run lint && npm run build && npm test",
    "ci:build": "npm run affected:build -- --base=master --head=HEAD",
    "ci:commit": "npm run ci:lint && npm run ci:build && npm run ci:test",
    "ci:e2e": "npm run affected:e2e -- --base=master --head=HEAD",
    "ci:lint": "npm run affected:lint -- --base=master --head=HEAD",
    "ci:push": "npm run lint && npm run ci:build && npm run ci:test && npm run ci:e2e",
    "ci:test": "npm run affected:test -- --base=master --head=HEAD",
    "ci": "npm run ci:all",
    "clean": "shx rm -Rf coverage dist",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "docs": "typedoc --options typedoc.json --exclude '**/*.test.ts' ./apps/ ./libs/",
    "e2e": "ng e2e",
    "format:check": "./node_modules/.bin/nx format:check",
    "format:write": "./node_modules/.bin/nx format:write",
    "format": "./node_modules/.bin/nx format:write",
    "git:commit": "git-cz",
    "help": "./node_modules/.bin/nx help",
    "lint:md": "markdownlint -i '**/node_modules/**' -c '.markdownlint.yaml' './'",
    "lint:sh": "docker pull koalaman/shellcheck:stable && docker run -v \"$PWD:/mnt\" koalaman/shellcheck ./bin/* && docker kill koalaman/shellcheck 2> /dev/null; docker rm -f koalaman/shellcheck 2> /dev/null; docker rmi -f koalaman/shellcheck:stable 2> /dev/null",
    "lint:styles": "sass-lint -v -q -f stylish",
    "lint:ts": "tslint \"**/*.ts\"",
    "lint": "npm run lint:md && npm run lint:styles && ng lint",
    "ng": "ng",
    "semantic-release": "semantic-release",
    "start": "ng serve",
    "test": "ng test",
    "uninstall": "npm run clean && shx rm -Rf node_modules",
    "update:check": "ng update",
    "update": "ng update @nrwl/schematics",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic"
  },
  "dependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/forms": "^6.1.10",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/router": "^6.1.10",
    "@ngrx/effects": "6.0.1",
    "@ngrx/router-store": "6.0.1",
    "@ngrx/store": "6.0.1",
    "@nrwl/nx": "6.3.0",
    "ajv": "^6.9.1",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.1",
    "@angular/cli": "6.1.2",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/language-service": "^6.1.10",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/travis-cli": "^7.5.2",
    "@ngrx/store-devtools": "6.0.1",
    "@nrwl/schematics": "6.3.0",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/exec": "^3.3.2",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/github": "^5.2.10",
    "@semantic-release/npm": "^5.1.4",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "acorn": "^6.1.0",
    "codelyzer": "~4.2.1",
    "commitizen": "^3.0.5",
    "commitlint-config-awesome": "^1.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "0.3.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "markdownlint-cli": "^0.13.0",
    "ng-packagr": "^4.7.1",
    "ngrx-store-freeze": "0.2.4",
    "prettier": "^1.16.4",
    "protractor": "~5.4.0",
    "sass-lint": "^1.12.1",
    "semantic-release": "^15.13.3",
    "semantic-release-monorepo": "^6.1.1",
    "shx": "^0.3.2",
    "ts-node": "~5.0.1",
    "tsickle": "^0.34.3",
    "tslint": "~5.9.1",
    "typedoc": "^0.13.0",
    "typescript": "~2.7.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "awesome"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run ci:commit",
      "pre-push": "npm run ci:push"
    }
  }
}
